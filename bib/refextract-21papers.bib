 link = {https://doi.org/10.3390/molecules24224114},
 month = {November},
 number = {22},
 pages = {4114},
 publisher = {MDPI AG},
 title = {Interfacial Behavior of Oligo(Ethylene Glycol) Dendrons Spread Alone and in Combination with a Phospholipid as Langmuir Monolayers at the Air/Water Interface},
 url = {http://dx.doi.org/10.3390/molecules24224114},
 volume = {24},
 year = {2019}
}

