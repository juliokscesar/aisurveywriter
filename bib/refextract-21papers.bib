 month = {June},
 number = {31},
 pages = {5429–5479},
 publisher = {Wiley},
 title = {Aggregation‐Induced Emission: The Whole Is More Brilliant than the Parts},
 url = {http://dx.doi.org/10.1002/adma.201401356},
