 publisher = {Oxford University Press (OUP)},
 title = {Charge Manipulation in Metal–Organic Frameworks: Toward Designer Functional Molecular Materials},
 url = {http://dx.doi.org/10.1246/bcsj.20210277},
 volume = {94},
 year = {2021}
}

