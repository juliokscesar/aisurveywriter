 month = {July},
 number = {5121},
 pages = {585–588},
 publisher = {American Association for the Advancement of Science (AAAS)},
 title = {Direct Colorimetric Detection of a Receptor-Ligand Interaction by a Polymerized Bilayer Assembly},
 url = {http://dx.doi.org/10.1126/science.8342021},
