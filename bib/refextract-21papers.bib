 month = {March},
 number = {5},
 pages = {333–343},
 publisher = {Wiley},
 title = {Interfaces Working for Biology: Solving Biological Mysteries and Opening Up Future Nanoarchitectonics},
 url = {http://dx.doi.org/10.1002/cnma.201600053},
 volume = {2},
 year = {2016}
}

