 doi = {10.15625/1811-4989/17109},
 issn = {1811-4989},
 journal = {Vietnam Journal of Biotechnology},
 link = {https://doi.org/10.15625/1811-4989/17109},
 month = {May},
 number = {4},
 pages = {749–754},
 publisher = {Publishing House for Science and Technology, Vietnam Academy of Science and Technology (Publications)},
 title = {Evaluation of nanosilver solution stability using UV-Vis spectral absorbency analysis and antibacterial activities on Escherichia coli and Staphylococcus aureus},
 url = {http://dx.doi.org/10.15625/1811-4989/17109},
 volume = {19},
 year = {2022}
}

@article{vjch.202000200,
