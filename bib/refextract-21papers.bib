 month = {July},
 number = {6},
 pages = {722–731},
 publisher = {Wiley},
 title = {<scp>CRISPR</scp>/<scp>Cas9</scp>‐mediated Nap knockout affects female reproduction and egg shape in Bombyx mori},
 url = {http://dx.doi.org/10.1111/imb.12937},
 volume = {33},
