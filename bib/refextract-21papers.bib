 link = {https://doi.org/10.1366/0003702934415273},
 month = {July},
 number = {7},
 pages = {869–874},
 publisher = {SAGE Publications},
 title = {Polarization-Modulated FT-IR Spectroscopy of a Spread Monolayer at the Air/Water Interface},
 url = {http://dx.doi.org/10.1366/0003702934415273},
 volume = {47},
 year = {1993}
}

