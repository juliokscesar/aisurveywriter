@article{adma.200802850,
 abstract = { Abstract 
Molecular electronics seeks to incorporate molecular components as functional elements in electronic devices. There are numerous strategies reported to date for the fabrication, design, and characterization of such devices, but a broadly accepted example showing structure\u2010dependent conductance behavior has not yet emerged. This progress report focuses on experimental methods for making both single\u2010molecule and ensemble molecular junctions, and highlights key results from these efforts. Based on some general objectives of the field, particular experiments are presented to show progress in several important areas, and also to define those areas that still need attention. Some of the variable behavior of ostensibly similar junctions reported in the literature is attributable to differences in the way the junctions are fabricated. These differences are due, in part, to the multitude of methods for supporting the molecular layer on the substrate, including methods that utilize physical adsorption and covalent bonds, and to the numerous strategies for making top contacts. After discussing recent experimental progress in molecular electronics, an assessment of the current state of the field is presented, along with a proposed road map that can be used to assess progress in the future. },
 author = {McCreery, Richard L. and Bergren, Adam Johan},
 doi = {10.1002/adma.200802850},
 issn = {1521-4095},
 journal = {Advanced Materials},
 link = {https://doi.org/10.1002/adma.200802850},
 month = {November},
 number = {43},
 pages = {4303–4322},
 publisher = {Wiley},
 title = {Progress with Molecular Electronic Junctions: Meeting Experimental Challenges in Design and Fabrication},
 url = {http://dx.doi.org/10.1002/adma.200802850},
 volume = {21},
 year = {2009}
}

