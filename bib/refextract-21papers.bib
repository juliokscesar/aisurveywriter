 month = {June},
 number = {2},
 pages = {46},
 publisher = {MDPI AG},
 title = {Iron Oxide Nanoparticles for Biomedical Applications: A Perspective on Synthesis, Drugs, Antimicrobial Activity, and Toxicity},
 url = {http://dx.doi.org/10.3390/antibiotics7020046},
