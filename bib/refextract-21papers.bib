@article{s19183839,
 abstract = {
The globalization of food distribution has made necessary to secure safe products to the general consumers through the rapid detection of harmful additives on the field. For this purpose, we developed a cuvette-type localized surface plasmon resonance (LSPR) sensor that can be easily used by consumers with conventional ultraviolet-visible light spectrophotometer for in-situ measurements. Gold nanoparticles were uniformly deposited on a transparent substrate via a self-assembly method to obtain a plasmonically active chip, and the chemical receptor p-nitroaniline (p-NA) was functionalized to stabilize the device sensitivity under external temperature and pH conditions. The fabricated chip was fixed onto a support and combined with a cuvette-type LSPR sensor. To evaluate the applicability of this sensor on the field, sensitivity and quantitative analysis experiments were conducted onto melamine as a model sample from harmful food additives. Under optimal reaction condition (2 mM p-NA for 20 min), we achieved an excellent detection limit (0.01 ppb) and a dynamic range allowing quantitative analysis over a wide concentration range (0.1\u20131000 ppb) from commercially available milk powder samples. },
 author = {Oh, Seo and Lee, Min and Heo, Nam and Kim, Suji and Oh, Jeong and Lee, Yuseon and Jeon, Eun and Moon, Hyungsil and Kim, Hyung and Park, Tae and Moon, Guiim and Chun, Hyang and Huh, Yun},
 doi = {10.3390/s19183839},
 issn = {1424-8220},
 journal = {Sensors},
 link = {https://doi.org/10.3390/s19183839},
 month = {September},
 number = {18},
 pages = {3839},
 publisher = {MDPI AG},
 title = {Cuvette-Type LSPR Sensor for Highly Sensitive Detection of Melamine in Infant Formulas},
 url = {http://dx.doi.org/10.3390/s19183839},
 volume = {19},
 year = {2019}
}

