 month = {December},
 number = {5},
 pages = {551–558},
 publisher = {Wiley},
 title = {Raman and ab initio study of the conformational isomerism in the 1‐ethyl‐3‐methyl‐imidazolium bis(trifluoromethanesulfonyl)imide ionic liquid},
 url = {http://dx.doi.org/10.1002/jrs.1680},
 volume = {38},
 year = {2006}
}

