 publisher = {Wiley},
 title = {Calix[4]Resorcinarene Macrocycles Interactions with Cd2+, Hg2+, Pb2+, and Cu2+ Cations: A QCM‐I and Langmuir Ultra‐thin Monolayers Study},
 url = {http://dx.doi.org/10.1002/elan.201900651},
 volume = {32},
