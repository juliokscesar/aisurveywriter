 pages = {693–699},
 publisher = {Oxford University Press (OUP)},
 title = {Inhibition of GLI-Mediated Transcription by Cyclic Pyrrole-Imidazole Polyamide in Cancer Stem Cells},
 url = {http://dx.doi.org/10.1246/bcsj.20210453},
 volume = {95},
