 doi = {10.1002/slct.202301571},
 issn = {2365-6549},
 journal = {ChemistrySelect},
 link = {https://doi.org/10.1002/slct.202301571},
 month = {July},
 number = {25},
 publisher = {Wiley},
 title = {Recent Breakthroughs and Future Potential of Inorganic Nanocomposites in Hydrogen Generation},
 url = {http://dx.doi.org/10.1002/slct.202301571},
 volume = {9},
 year = {2024}
}

