 publisher = {Walter de Gruyter GmbH},
 title = {Nonspecific sensor arrays (“electronic tongue”) for chemical analysis of liquids (IUPAC Technical Report)},
 url = {http://dx.doi.org/10.1351/pac200577111965},
 volume = {77},
 year = {2005}
}

