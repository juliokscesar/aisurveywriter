 month = {March},
 number = {10},
 pages = {3892–3898},
 publisher = {Wiley},
 title = {Rapid and sensitive surface‐enhanced Raman spectroscopy (SERS) method combined with gold nanoparticles for determination of paraquat in apple juice},
 url = {http://dx.doi.org/10.1002/jsfa.8906},
 volume = {98},
 year = {2018}
}

