@article{a-1920-4161,
 abstract = { Abstract 
Gastritis cystica profunda (GCP) is a rare disease that is often observed at the stoma site of patients undergoing\n      gastric surgery. Specific symptoms are often lacking, making it difficult to diagnose. GCP may\n      develop into cancer. Endoscopic resection is a good method to reduce surgical injury, and the\n      therapeutic effect depends on the status of the primary disease. 
The patient was a 77-year-old male who was admitted to the hospital for repeated\n      epigastric distention. The man had never undergone gastric surgery and did not have Helicobacter pylori infection. Physical and laboratory tests showed no\n      abnormality. Computed tomography showed no significant abnormality. Endoscopy revealed\n      multiple ulcers on the posterior wall of the gastric body, but biopsy showed no evidence of\n      malignancy. The patient subsequently underwent ESD surgery with a lesion of 1.5 cm\u00d72.3 cm.\n      Pathological results showed the cystic dilatation of gastric glands in the submucosa, with\n      some well-differentiated adenocarcinoma. The final diagnosis was GCP with well-differentiated\n      gastric adenocarcinoma. 
Herein, we describe a case of ESD therapy for GCP with well-differentiated gastric\n      adenocarcinoma. When gastric mucosal lesions are difficult to diagnose, further treatment and\n      diagnosis with EMR or ESD can be considered. For patients with early gastric cancer, long-term\n      postoperative monitoring should be carried out. },
 author = {Huang, Cao and Liu, Jiang},
 doi = {10.1055/a-1920-4161},
 issn = {1439-7803},
 journal = {Zeitschrift für Gastroenterologie},
 link = {https://doi.org/10.1055/a-1920-4161},
 month = {February},
 publisher = {Georg Thieme Verlag KG},
 title = {Well-differentiated Adenocarcinoma Arising from Gastric Cystica Profunda Treated by Endoscopic Submucosal Dissection: A case report and literature review},
 url = {http://dx.doi.org/10.1055/a-1920-4161},
 year = {2025}
}

