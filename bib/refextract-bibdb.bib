 month = {February},
 pages = {6},
 publisher = {Vien Y hoc Du phong Quan Doi, Cuc Quan Y},
 title = {EVALUATION OF EMOTIONAL DISTRESS IN A COHORT OF NEWLY DIAGNOSED CANCER PATIENTS: A STUDY OF 67 INDIVIDUAS},
 url = {http://dx.doi.org/10.59459/1859-1655/jmm.346},
 year = {2024}
}

@article{jmr.2002.0014,
 abstract = {
