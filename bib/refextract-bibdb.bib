@article{bcsj.20220071,
 abstract = { Abstract 
Promoted understanding of nanostructures and their functions significantly rely on rapid progress of nanotechnology within a few decades. It would be a fruitful way to consider materials science through regulation of nanostructure regulations on the basis of nanotechnological accomplishments. As a post-nanotechnology concept, a new concept of nanoarchitectonics has recently emerged as a methodology to produce functional materials from nanoscale units. In this review article, we discuss the recent advances of materials nanoarchitectonics and related approaches from atom-scales to living cell sizes. The approaches on multiscale nanoarchitectonics are exemplified and discussed including atom and molecules nanoarchitectonics, supramolecular nanoarchitectonics, and nanoarchitectonics at cell-scale. Next, supracell-level nanoarchitectonics is discussed including living cell organization at designed surfaces, cell sheets and layered nanoarchitectonics, three-dimensional cell nanoarchitectonics, biofilms and bioinks, and new challenges for living cells at liquid interface. All the materials are formed with fundamental units, atoms, molecules, and nanomaterials, and therefore, the nanoarchitectonics approaches from basic units to materials can be hypothetically applied to any kind of material. Therefore, nanoarchitectonics could be a method for everything in materials science as an analogy to a theory of everything in physics. },
 author = {Ariga, Katsuhiko and Fakhrullin, Rawil},
 doi = {10.1246/bcsj.20220071},
 issn = {1348-0634},
 journal = {Bulletin of the Chemical Society of Japan},
 link = {https://doi.org/10.1246/bcsj.20220071},
 month = {March},
 number = {5},
 pages = {774–795},
 publisher = {Oxford University Press (OUP)},
 title = {Materials Nanoarchitectonics from Atom to Living Cell: A Method for Everything},
 url = {http://dx.doi.org/10.1246/bcsj.20220071},
 volume = {95},
 year = {2022}
}

@article{bcsj.20220086,
 abstract = { Abstract \n               
We aim to establish the importance of molecular design for the formation of monolayers at an air-water interface within the concept \u201ccoordination amphiphile\u201d, which is based on ligand characteristics and molecular topology. For this purpose, five types of platinum complexes containing a coordination plane, including salicylaldiminato (SA) and \u03b2-(iminomethyl)azolato (IA) complexes, were prepared where the ligand characteristics were controlled. Polymethylene-vaulted and non-vaulted complexes were then examined to assess the effects of molecular topology on interfacial activity. SA complexes tend to undergo random aggregation at an air-water interface, while the weak hydrophilicity of SA can assist in the formation of a stable monolayer if hydrophobic and hydrophilic chains are introduced to the structure. In contrast, IA complexes exhibit topological specificity; imidazolato and pyrazolato complexes form monolayers only for non-vaulted and vaulted complex, respectively. Molecular modelling and association constants of the compounds suggest that an appropriate hydrophilicity of the coordination plane and intermolecular interactions involving hydrogen bonding are important factors for monolayer formation. },
 author = {Adachi, Junya and Naito, Masaya and Sugiura, Sho and Le, Ngoc Ha-Thu and Nishimura, Shoma and Huang, Shufang and Suzuki, Shuichi and Kawamorita, Soichiro and Komiya, Naruyoshi and Hill, Jonathan P and Ariga, Katsuhiko and Naota, Takeshi and Mori, Taizo},
