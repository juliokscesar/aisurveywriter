 publisher = {Wiley},
 title = {Synthesis of C−C Bonded Two‐Dimensional Conjugated Covalent Organic Framework Films by Suzuki Polymerization on a Liquid–Liquid Interface},
 url = {http://dx.doi.org/10.1002/anie.201811399},
 volume = {58},
 year = {2019}
}

